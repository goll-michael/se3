<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3 http://maven.apache.org/xsd/assembly-1.1.3.xsd">
	<id>descriptor-client</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/</outputDirectory>
			<useProjectArtifact>true</useProjectArtifact>
			<unpack>true</unpack>
			<scope>runtime</scope>
			<unpackOptions>
				<excludes>
					<exclude>server/ServerStart*.class</exclude>
					<exclude>server/application</exclude>
					<exclude>server/application/Kunde.class</exclude>
					<exclude>server/application/KundenContainerEinfach.class</exclude>
					<exclude>server/application/KundennummerRessource.class</exclude>
					<exclude>server/application/KundenRessource.class</exclude>
					<exclude>server/persistence/**</exclude>
				</excludes>
			</unpackOptions>
		</dependencySet>
	</dependencySets>
</assembly>